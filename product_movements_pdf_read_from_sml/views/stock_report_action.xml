<odoo>
    <record id="stock_report_landscape" model="report.paperformat">
        <field name="name">Stock Report Landscape</field>
        <field name="format">A4</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">5</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
    </record>

    <report
        id="stock_report_action"
        model="stock.report.wizard"
        string="Stock Report"
        report_type="qweb-pdf"
        name="product_movements_pdf.stock_report_template"
        file="product_movements_pdf.stock_report_template"
        attachment_use="False"
        paperformat="product_movements_pdf.stock_report_landscape"
    />
</odoo>
